<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="deri.sensor.javabeans">
	<class name="FlightRoute" table="DERI.DBA.flightroutecsv">
		<id name="id">
		</id>		
		<property name="CallSign" type="string" length="15"/>
		<property name="FlightNumber" type="string" length="15"/>	
		<property name="Departure" type="string" length="5"/>
		<property name="Transit" type="string" length="20"/>
		<property name="Destination" type="string" length="5"/>
	</class>
</hibernate-mapping>